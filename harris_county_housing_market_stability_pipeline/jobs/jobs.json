{
  "name": "h_c_property_job",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "notification_settings": {
    "no_alert_for_skipped_runs": false,
    "no_alert_for_canceled_runs": false
  },
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "init_schema",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "notebooks/init_schema"
      }
    },
    {
      "task_key": "bronze_owners",
      "depends_on": [{ "task_key": "init_schema" }],
      "notebook_task": {
        "notebook_path": "notebooks/bronze/owners_bronze"
      }
    },
    {
      "task_key": "bronze_property",
      "depends_on": [{ "task_key": "init_schema" }],
      "notebook_task": {
        "notebook_path": "notebooks/bronze/property_bronze"
      },
      "environment_key": "bronze_property_environment"
    },
    {
      "task_key": "bronze_zip_code",
      "depends_on": [{ "task_key": "init_schema" }],
      "notebook_task": {
        "notebook_path": "notebooks/bronze/zip_codes_bronze"
      }
    },
    {
      "task_key": "year_parameterization",
      "depends_on": [
        { "task_key": "bronze_owners" },
        { "task_key": "bronze_property" },
        { "task_key": "bronze_zip_code" }
      ],
      "for_each_task": {
        "inputs": "[2017,2018,2019,2020,2021,2022,2023,2024,2025]",
        "task": {
          "task_key": "year_parameterization_iteration",
          "notebook_task": {
            "notebook_path": "notebooks/silver/joined_silver"
          }
        }
      }
    },
    {
      "task_key": "scd_creation",
      "depends_on": [{ "task_key": "year_parameterization" }],
      "notebook_task": {
        "notebook_path": "notebooks/gold/scd_gold"
      }
    },
    {
      "task_key": "gold_table_creation",
      "depends_on": [{ "task_key": "scd_creation" }],
      "notebook_task": {
        "notebook_path": "notebooks/gold/Gold_layer"
      }
    }
  ],
  "queue": { "enabled": true },
  "environments": [
    {
      "environment_key": "bronze_property_environment",
      "spec": { "environment_version": "4" }
    }
  ],
  "performance_target": "PERFORMANCE_OPTIMIZED"
}
