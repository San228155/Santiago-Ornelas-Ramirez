{
  "name": "h_c_property_job",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "notification_settings": {
    "no_alert_for_skipped_runs": false,
    "no_alert_for_canceled_runs": false
  },
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "init_schema",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/h_c_notebooks/init_schema",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "year_parameterization_bronze_owners",
      "depends_on": [
        {
          "task_key": "init_schema"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "[2017,2018,2019,2020,2021,2022,2023,2024,2025]",
        "concurrency": 4,
        "task": {
          "task_key": "year_parameterization_bronze_owners_notebook",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/h_c_notebooks/bronze/owners_bronze",
            "base_parameters": {
              "year": "{{input}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "year_parameterization_bronze_property",
      "depends_on": [
        {
          "task_key": "init_schema"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "[2017,2018,2019,2020,2021,2022,2023,2024,2025]",
        "concurrency": 4,
        "task": {
          "task_key": "bronze_property_iteration",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/h_c_notebooks/bronze/property_bronze",
            "base_parameters": {
              "year": "{{input}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {},
          "environment_key": "bronze_property_environment"
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "year_parameterization_bronze_zip_code",
      "depends_on": [
        {
          "task_key": "init_schema"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "[2017,2018,2019,2020,2021,2022,2023,2024,2025]",
        "concurrency": 4,
        "task": {
          "task_key": "year_parameterization_bronze_zip_codes",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/h_c_notebooks/bronze/zip_codes_bronze",
            "base_parameters": {
              "year": "{{input}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "year_parameterization",
      "depends_on": [
        {
          "task_key": "year_parameterization_bronze_property"
        },
        {
          "task_key": "year_parameterization_bronze_owners"
        },
        {
          "task_key": "year_parameterization_bronze_zip_code"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "[2017,2018,2019,2020,2021,2022,2023,2024,2025]",
        "concurrency": 1,
        "task": {
          "task_key": "year_parameterization_iteration",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/h_c_notebooks/silver/joined_silver",
            "base_parameters": {
              "year": "{{input}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "scd_creation",
      "depends_on": [
        {
          "task_key": "year_parameterization"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/h_c_notebooks/gold/scd_gold",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "gold_table_creation",
      "depends_on": [
        {
          "task_key": "scd_creation"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/h_c_notebooks/gold/Gold_layer",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "environments": [
    {
      "environment_key": "bronze_property_environment",
      "spec": {
        "environment_version": "4"
      }
    }
  ],
  "performance_target": "PERFORMANCE_OPTIMIZED"
}
